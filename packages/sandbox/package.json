{
  "name": "@agentstart/sandbox",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./e2b": "./dist/e2b.mjs",
    "./nodejs": "./dist/nodejs.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "biome check --write --unsafe ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@agentstart/types": "workspace:*",
    "@agentstart/utils": "workspace:*",
    "@e2b/code-interpreter": "catalog:",
    "chokidar": "catalog:",
    "fast-glob": "catalog:",
    "fs-extra": "catalog:",
    "nanoid": "catalog:",
    "pathe": "catalog:"
  },
  "devDependencies": {
    "@agentstart/biome-config": "workspace:*",
    "@agentstart/tsconfig": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@types/fs-extra": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./e2b": "./dist/e2b.mjs",
      "./nodejs": "./dist/nodejs.mjs",
      "./package.json": "./package.json"
    },
    "access": "public"
  }
}
