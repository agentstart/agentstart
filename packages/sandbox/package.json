{
  "name": "@agent-stack/sandbox",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "bunx biome check --write ./src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agent-stack/infra": "workspace:*",
    "@e2b/code-interpreter": "catalog:",
    "chokidar": "catalog:",
    "execa": "catalog:",
    "fast-glob": "catalog:",
    "nanoid": "catalog:",
    "path-browserify-esm": "catalog:"
  },
  "devDependencies": {
    "@agent-stack/biome-config": "workspace:*",
    "@agent-stack/tsconfig": "workspace:*",
    "@biomejs/biome": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  }
}
