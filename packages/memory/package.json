{
  "name": "@agentstart/memory",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "bunx biome check --write --unsafe ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun run prisma:push && vitest",
    "prisma:push": "prisma db push --schema src/memory/prisma/test/schema.prisma",
    "test:adapters": "bun run prisma:push && vitest adapters"
  },
  "dependencies": {
    "@agentstart/types": "workspace:*",
    "@agentstart/utils": "workspace:*",
    "@prisma/client": "catalog:",
    "better-sqlite3": "catalog:",
    "drizzle-orm": "catalog:",
    "kysely": "catalog:",
    "mongodb": "catalog:",
    "mysql2": "catalog:",
    "pg": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "ioredis": "^5.0.0",
    "@vercel/kv": "^3.0.0",
    "@upstash/redis": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    },
    "@vercel/kv": {
      "optional": true
    },
    "@upstash/redis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@agentstart/biome-config": "workspace:*",
    "@agentstart/tsconfig": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@types/better-sqlite3": "catalog:",
    "@types/pg": "catalog:",
    "@upstash/redis": "^1.0.0",
    "@vercel/kv": "^3.0.0",
    "ioredis": "catalog:",
    "prisma": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  }
}
