{
  "name": "agent-stack",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./db": {
      "import": {
        "types": "./dist/db/index.d.ts",
        "default": "./dist/db/index.js"
      },
      "require": {
        "types": "./dist/db/index.d.cts",
        "default": "./dist/db/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.js"
      },
      "require": {
        "types": "./dist/client/index.d.cts",
        "default": "./dist/client/index.cjs"
      }
    },
    "./adapter": {
      "import": {
        "types": "./dist/adapter/index.d.ts",
        "default": "./dist/adapter/index.js"
      },
      "require": {
        "types": "./dist/adapter/index.d.cts",
        "default": "./dist/adapter/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "bunx biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun run prisma:push && vitest",
    "prisma:push": "prisma db push --schema src/adapter/prisma/test/schema.prisma"
  },
  "dependencies": {
    "@agent-stack/api": "*",
    "@agent-stack/agent": "*",
    "@agent-stack/infra": "*",
    "@agent-stack/integration": "*",
    "@agent-stack/client": "*",
    "@agent-stack/sandbox": "*",
    "@agent-stack/utils": "*"
  },
  "devDependencies": {
    "@agent-stack/biome-config": "*",
    "@agent-stack/tsconfig": "*",
    "@biomejs/biome": "catalog:",
    "@prisma/client": "catalog:",
    "@types/better-sqlite3": "catalog:",
    "@types/pg": "catalog:",
    "better-sqlite3": "catalog:",
    "drizzle-kit": "catalog:",
    "drizzle-orm": "catalog:",
    "kysely": "catalog:",
    "mongodb": "catalog:",
    "mysql2": "catalog:",
    "pg": "catalog:",
    "prisma": "catalog:",
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "ai": "^5"
  }
}
