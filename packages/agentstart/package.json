{
  "name": "agentstart",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./agent": {
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.cjs"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./db": {
      "import": "./dist/db/index.js",
      "require": "./dist/db/index.cjs"
    },
    "./integration": {
      "import": "./dist/integration/index.js",
      "require": "./dist/integration/index.cjs"
    },
    "./kv": {
      "import": "./dist/kv/index.js",
      "require": "./dist/kv/index.cjs"
    },
    "./sandbox": {
      "import": "./dist/sandbox/index.js",
      "require": "./dist/sandbox/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "bunx biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun run prisma:push && vitest",
    "prisma:push": "prisma db push --schema src/db/adapter/prisma/test/schema.prisma"
  },
  "dependencies": {
    "@agentstart/utils": "workspace:*",
    "@ai-sdk/react": "catalog:",
    "@e2b/code-interpreter": "catalog:",
    "@orpc/client": "catalog:",
    "@orpc/server": "catalog:",
    "chokidar": "catalog:",
    "fast-glob": "catalog:",
    "nanoid": "catalog:",
    "pathe": "catalog:",
    "redis": "catalog:",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@agentstart/biome-config": "workspace:*",
    "@agentstart/tsconfig": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@prisma/client": "catalog:",
    "@types/better-sqlite3": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/pg": "catalog:",
    "@types/react": "catalog:",
    "better-sqlite3": "catalog:",
    "drizzle-kit": "catalog:",
    "drizzle-orm": "catalog:",
    "kysely": "catalog:",
    "mongodb": "catalog:",
    "mysql2": "catalog:",
    "pg": "catalog:",
    "prisma": "catalog:",
    "react": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "ai": "^5",
    "react": "^19"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "typesVersions": {
    "*": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./agent": {
        "import": "./dist/agent/index.js",
        "require": "./dist/agent/index.cjs"
      },
      "./api": {
        "import": "./dist/api/index.js",
        "require": "./dist/api/index.cjs"
      },
      "./client": {
        "import": "./dist/client/index.js",
        "require": "./dist/client/index.cjs"
      },
      "./db": {
        "import": "./dist/db/index.js",
        "require": "./dist/db/index.cjs"
      },
      "./integration": {
        "import": "./dist/integration/index.js",
        "require": "./dist/integration/index.cjs"
      },
      "./kv": {
        "import": "./dist/kv/index.js",
        "require": "./dist/kv/index.cjs"
      },
      "./sandbox": {
        "import": "./dist/sandbox/index.js",
        "require": "./dist/sandbox/index.cjs"
      },
      "./package.json": "./package.json"
    }
  }
}
