{
  "name": "agentstart",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./agent": {
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.cjs"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs"
    },
    "./blob": {
      "import": "./dist/blob/index.js",
      "require": "./dist/blob/index.cjs"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./integration": {
      "import": "./dist/integration/index.js",
      "require": "./dist/integration/index.cjs"
    },
    "./memory": {
      "import": "./dist/memory/index.js",
      "require": "./dist/memory/index.cjs"
    },
    "./sandbox": {
      "import": "./dist/sandbox/index.js",
      "require": "./dist/sandbox/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun run generate:sdk:docs && tsdown",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "bunx biome check --write --unsafe ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "generate:sdk:docs": "bun run scripts/generate-sdk-docs.ts"
  },
  "dependencies": {
    "@agentstart/blob": "workspace:*",
    "@agentstart/memory": "workspace:*",
    "@agentstart/types": "workspace:*",
    "@agentstart/utils": "workspace:*",
    "@ai-sdk/react": "catalog:",
    "@e2b/code-interpreter": "catalog:",
    "@orpc/client": "catalog:",
    "@orpc/server": "catalog:",
    "@orpc/tanstack-query": "catalog:",
    "@tanstack/react-query": "catalog:",
    "chokidar": "catalog:",
    "fast-glob": "catalog:",
    "nanoid": "catalog:",
    "pathe": "catalog:",
    "tokenlens": "catalog:",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@agentstart/biome-config": "workspace:*",
    "@agentstart/tsconfig": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@prisma/client": "catalog:",
    "@types/better-sqlite3": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/pg": "catalog:",
    "@types/react": "catalog:",
    "better-sqlite3": "catalog:",
    "drizzle-kit": "catalog:",
    "drizzle-orm": "catalog:",
    "ai": "catalog:",
    "kysely": "catalog:",
    "mongodb": "catalog:",
    "mysql2": "catalog:",
    "pg": "catalog:",
    "react": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "ai": "^6",
    "react": "^19"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "typesVersions": {
    "*": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./agent": {
        "import": "./dist/agent/index.js",
        "require": "./dist/agent/index.cjs"
      },
      "./api": {
        "import": "./dist/api/index.js",
        "require": "./dist/api/index.cjs"
      },
      "./client": {
        "import": "./dist/client/index.js",
        "require": "./dist/client/index.cjs"
      },
      "./usage": {
        "import": "./dist/usage/index.js",
        "require": "./dist/usage/index.cjs"
      },
      "./memory": {
        "import": "./dist/memory/index.js",
        "require": "./dist/memory/index.cjs"
      },
      "./integration": {
        "import": "./dist/integration/index.js",
        "require": "./dist/integration/index.cjs"
      },
      "./sandbox": {
        "import": "./dist/sandbox/index.js",
        "require": "./dist/sandbox/index.cjs"
      },
      "./package.json": "./package.json"
    }
  }
}
