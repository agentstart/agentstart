{
  "name": "@agentstart/cli",
  "version": "0.1.0",
  "description": "The CLI for Agent Start",
  "module": "./dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/xinyao27/agentstart",
    "directory": "packages/cli"
  },
  "main": "./dist/index.mjs",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "start": "node ./dist/index.mjs",
    "test": "vitest",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "bunx biome check --write ./src",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "executableFiles": [
      "./dist/index.mjs"
    ],
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "bin": "./dist/index.mjs",
  "devDependencies": {
    "@agentstart/biome-config": "workspace:*",
    "@agentstart/tsconfig": "workspace:*",
    "@types/fs-extra": "catalog:",
    "@types/semver": "catalog:",
    "diff": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@agentstart/utils": "workspace:*",
    "@babel/preset-react": "catalog:",
    "@babel/preset-typescript": "catalog:",
    "@clack/prompts": "catalog:",
    "@mrleebo/prisma-ast": "catalog:",
    "@prisma/client": "catalog:",
